[settings]
os=Linux
arch=armv8
compiler=clang
compiler.version=17
compiler.libcxx=libstdc++11
build_type=Release

[conf]
# tell conan how to call cross compilers (this helps some tools)
tools.build:compiler_executables={
    "c":"clang",
    "cpp":"clang++",
    "ar":"llvm-ar",
    "ranlib":"llvm-ranlib"
}
