[settings]
os=Linux
arch=armv8
compiler=gcc
compiler.version=13
compiler.libcxx=libstdc++11
build_type=Release

[conf]
tools.build:compiler_executables={"c":"aarch64-linux-gnu-gcc","cpp":"aarch64-linux-gnu-g++","ar":"aarch64-linux-gnu-ar","ranlib":"aarch64-linux-gnu-ranlib"}
tools.cmake.cmaketoolchain:extra_variables={
 "TD_INSTALL_STATIC_LIBRARIES":"ON",
 "TD_INSTALL_SHARED_LIBRARIES":"ON",
 "TD_ENABLE_LTO":"OFF",
 "TD_ENABLE_JNI":"OFF"
}
